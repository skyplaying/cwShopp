<view class="page"><block wx:if="{{isok}}"><view class="container"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="selectBox" bindtap="__e"><text class="text1">{{selectext?selectext:'已上架'}}</text><image class="bottomArrows" src="../../../static/shop/bottomJianTou.png" mode="aspectFit"></image></view><block wx:if="{{isSelect}}"><view class="selectPop"><block wx:for="{{goodStatusList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view data-event-opts="{{[['tap',[['selectService',['$0'],[[['goodStatusList','',index]]]]]]]}}" class="s1" bindtap="__e">{{item}}</view></view></block></view></block><view data-event-opts="{{[['tap',[['addGoods',['$event']]]]]}}" class="addBtn" bindtap="__e">+添加</view><block wx:for="{{goodsInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="content"><view class="serviceModel"><view class="info"><image class="serviceImg" src="{{item.goodsImageLink}}" mode="aspectFill"></image><view class="fontSymbol"><text class="name">{{item.goodsName}}</text><view class="price">{{"￥"+item.goodsPrice}}</view></view></view><view class="btnBox"><view data-event-opts="{{[['tap',[['deleteService',['$0'],[[['goodsInfo','',index,'goodsId']]]]]]]}}" bindtap="__e">删除</view><view data-event-opts="{{[['tap',[['putService',['$0'],[[['goodsInfo','',index]]]]]]]}}" style="{{(item.goodsState==1?putUp:putDown)}}" bindtap="__e">{{item.goodsState==1?'上架':'下架'}}</view><view data-event-opts="{{[['tap',[['editor',['$0'],[[['goodsInfo','',index]]]]]]]}}" bindtap="__e">编辑</view></view></view></view></block></view></block></view>