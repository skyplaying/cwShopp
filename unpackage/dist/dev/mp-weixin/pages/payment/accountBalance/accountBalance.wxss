.content {
  width: 100%;
  padding: 30rpx;
}
.transDetailList {
  margin-top: 80rpx;
}
.transDetailBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 40rpx 0;
  width: 100%;
  color: #101010;
  font-size: 28rpx;
  vertical-align: middle;
  font-family: Arial;
}
.transDetailBox .moneyCount {
  color: #101010;
  font-size: 36rpx;
  text-align: right;
  font-family: Arial;
  position: absolute;
  right: 30rpx;
  margin-top: 10rpx;
}
.transDetailBox .payinfo .t1 {
  color: #101010;
  font-size: 32rpx;
  text-align: left;
  font-family: Arial;
}
.transDetailBox .payinfo .t2 {
  color: #999999;
  font-size: 24rpx;
  text-align: left;
  font-family: Arial;
  margin-top: 8rpx;
}
.transDetailBox .userIcon {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.getTip {
  position: absolute;
  right: 30rpx;
  width: 192rpx;
  height: 74rpx;
  color: #888888;
  font-size: 24rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.units {
  position: relative;
  left: 32rpx;
  width: 168rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #101010;
  font-size: 28rpx;
  font-family: Microsoft Yahei;
  border: 2rpx solid rgba(255, 255, 255, 0);
}
.units .bottomArrows {
  width: 15rpx;
  height: 15rpx;
  position: absolute;
  left: 135rpx;
  top: 18rpx;
}
.selectPop {
  width: 168rpx;
  border: 1rpx solid #888888;
  display: inline-block;
  background: white;
  z-index: 50;
  position: absolute;
}
.selectPop .s1 {
  width: 143rpx;
  height: 35rpx;
  text-align: left;
  font-size: 25rpx;
  line-height: 35rpx;
  border: none;
  padding-left: 25rpx;
}
.selectPop .s1:hover {
  background: #108ee9;
  color: white;
}

